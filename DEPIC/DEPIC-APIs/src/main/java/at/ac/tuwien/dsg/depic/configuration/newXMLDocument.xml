<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DataElasticityRequirement>
    <DataElasticityMetric xsi:type="NumericalElasticityMetric" 
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        ...
        <ElasticityMetricName>DataAccuracy</ElasticityMetricName>
        ...
        
        <elastictyBoundary>
            <lowerBoundary>10.0</lowerBoundary>
            <upperBoundary>90.0</upperBoundary>
        </elastictyBoundary>
        
        <greaterThanValue>15.0</greaterThanValue>
        <lessThanValue>85.0</lessThanValue>
        <priorityOrder>3</priorityOrder>
        
    </DataElasticityMetric>
    <DataElasticityMetric xsi:type="CategoricalElasticityMetric" 
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        ...
    </DataElasticityMetric>
</DataElasticityRequirement>
